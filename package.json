{
  "name": "ts-vue-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:cloud": "vue-cli-service serve --mode cloud",
    "dev:site": "vue-cli-service serve --mode site",
    "dev:company": "vue-cli-service serve --mode company",
    "build:cloud": "vue-cli-service build --mode procloud --NODE_ENV production",
    "build:site": "vue-cli-service build --mode prosite --NODE_ENV production",
    "build:company": "vue-cli-service build --mode procompany --NODE_ENV production",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@types/crypto-js": "^3.1.43",
    "@types/echarts": "^4.1.9",
    "@types/js-cookie": "^2.2.2",
    "awe-dnd": "^0.3.4",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "clipboard": "^2.0.4",
    "compressorjs": "^1.0.6",
    "core-js": "^2.6.5",
    "crypto-js": "^3.1.9-1",
    "echarts": "^4.2.1",
    "hls.js": "^0.12.4",
    "iview": "^3.4.1",
    "js-cookie": "^2.2.0",
    "moment": "^2.24.0",
    "qrcodejs2": "0.0.2",
    "qs": "^6.7.0",
    "style-resources-loader": "^1.2.1",
    "uuid": "^3.3.3",
    "v-distpicker": "^1.2.1",
    "video.js": "^7.5.4",
    "vue": "^2.6.10",
    "vue-aplayer": "^1.6.1",
    "vue-class-component": "^7.0.2",
    "vue-cropperjs": "^4.0.1",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vue-video-player": "^5.0.2",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.2",
    "webpack-cli": "^3.3.5",
    "worker-loader": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-typescript": "^3.7.0",
    "@vue/cli-plugin-unit-jest": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-plugin-import": "^1.11.0",
    "compression-webpack-plugin": "^3.0.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "sass": "^1.18.0",
    "sass-loader": "^7.1.0",
    "ts-jest": "^23.0.0",
    "typescript": "^3.4.3",
    "vue-template-compiler": "^2.5.21",
    "vuex-module-decorators": "^0.9.9",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "ie 10",
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": true
      }
    }
  }
}
